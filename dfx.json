{
  "canisters": {
    "elp_frontend": {
      "frontend": {
        "entrypoint": "elp_frontend/src/index.html"
      },
      "source": [
        "elp_frontend/assets",
        "dist/elp_frontend/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "canister_http": {
        "enabled": true
      },
      "replica": {
        "subnet_type": "system"
      },
      "type": "ephemeral"
    }
  },
  "version": 1
}